        &lt;script&gt;function trim(value) {
    var s = &amp;quot;&amp;quot; + value;
    return s.replace(/^\s*|\s*$/g, &amp;quot;&amp;quot;); 
}

function trimArrayElements (arr){
	for (var i=0;i &amp;lt; arr.length; i++) {
		arr[i] = arr[i].replace(/^\s\s*/, &amp;apos;&amp;apos;).replace(/\s\s*$/, &amp;apos;&amp;apos;);
	}
	return arr;	
}

function hasCommonElement(arr1, arr2) { 
	var obj = {};        
	for (var i = 0; i &amp;lt; arr1.length; i++) { 
		if(!obj[arr1[i]]) { 	
			var element = arr1[i]; 
			obj[element] = true; 
		} 
	} 
	for (var j = 0; j &amp;lt; arr2.length; j++) {   
		if(obj[arr2[j]]) { 
			return true; 
		} 
	}	 
	return false; 
} 

function hasDupElement(arr) { 
	var obj = {};        
	for (var i = 0; i &amp;lt; arr.length; i++) { 
		if(obj[arr[i]]) { 	
			return true;			 
		} 
		else {
			var element = arr[i]; 
			obj[element] = true;
		}
	} 		 
	return false; 
}

scriptletResponse = &amp;quot;success&amp;quot;;
var actionListHide = scriptletContext.get(&amp;quot;actionListHide&amp;quot;);
var actionListHideArr = [];
if (actionListHide &amp;amp;&amp;amp; trim(actionListHide))
{
	actionListHideArr = actionListHide.split(&amp;quot;,&amp;quot;);
	if (actionListHideArr.length &amp;gt; 0){
		actionListHideArr = trimArrayElements(actionListHideArr);
		scriptletContext.put(&amp;quot;actionListHideArr&amp;quot;,actionListHideArr);		
	}	
}

var actionListExpose = scriptletContext.get(&amp;quot;actionListExpose&amp;quot;);
var actionListExposeArr = [];
if (actionListExpose &amp;amp;&amp;amp; trim(actionListExpose))
{
	actionListExposeArr = actionListExpose.split(&amp;quot;,&amp;quot;);
	if (actionListExposeArr.length &amp;gt; 0){
		actionListExposeArr = trimArrayElements(actionListExposeArr);
		scriptletContext.put(&amp;quot;actionListExposeArr&amp;quot;,actionListExposeArr);			
	}	
}

if (actionListHideArr.length == 0 &amp;amp;&amp;amp; actionListExposeArr.length == 0)
{
	scriptletResponse = &amp;quot;failure&amp;quot;;
}
else
{
	if (hasCommonElement(actionListExposeArr,actionListHideArr)){
		scriptletResponse = &amp;quot;failure&amp;quot;;	
	}
	else {
		if (actionListHideArr.length &amp;gt; 0){
			if (hasDupElement(actionListHideArr)){
				scriptletResponse = &amp;quot;failure&amp;quot;;	
			}				
		}		 
		if (scriptletResponse == &amp;quot;success&amp;quot; &amp;amp;&amp;amp; actionListExposeArr.length &amp;gt; 0){
			if (hasDupElement(actionListExposeArr)){
				scriptletResponse = &amp;quot;failure&amp;quot;;	
			}			
		}		
	}
}&lt;/script&gt;
