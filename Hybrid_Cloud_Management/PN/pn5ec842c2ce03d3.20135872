<strong>PowerShell Script - Get-VMWebConsoleURL.ps1</strong>

<pre><code># File name: Get-VMWebConsoleURL.ps1
#
# Purpose: 1. Returns VM Web Console URL. This web console URL can be used in a web browser to connect to VM console.
#          2. Use as-is. Not officially supported through Micro Focus.
#
# Limitations: 1. Does not use proxy. If your environment requires proxy to 
#                 connect to the vCenter Application, enhance this script
#                 to accommodate vCenter connection requests through proxy. 
#
# Usage: 1. Runs on Windows OS and it requires Windows Powershell with vCenter Power CLI module.
#           Copy this script to a directory on the windows server.
#        2. Open powershell window 
#        3. Run Get-Help .\Get-VMWebConsoleURL.ps1 to get a list of parameters that
#           needs to be passed.
#        4. Sample usage: .\Get-VMWebConsoleURL.ps1 -VcenterHost 
#                         vCenter.FQDN.com -VcenterPort 443 -VcenterUser admin -VcenterPwd &lt;AdminPassword&gt; 
#                         -VcenterWebConsolePort 9443 -VmId vm-1234 -VcenterWebConsoleLocale en_US 
#        5. Parameter explanation:
#           VcenterHost:               Fully qualified domain name of vCenter server host.
#                                      VcenterHost parameter is mandatory. 
#           VcenterPort:               vCenter Server port number. By default, script uses 443 for vCenter server port number.
#                                      VcenterPort is optional.
#           VcenterUser:               vCenter login name with privileges to connect and retrieve VM information. 
#                                      VcenterUser parameter is mandatory.
#           VcenterPasswd:             VcenterUser's password to connect to vCenter. 
#                                      VcenterPwd parameter is mandatory.
#           VcenterWebConsolePort:     vCenter VM Web Console port. By default, script uses 9443 for vCenter web console port.
#                                      VcenterWebConsolePort parameter is optional.
#           VmId:                      ID of virtual machine for which web console URL has to be generated. 
#                                      VmId parameter is mandatory.
#           VcenterWebConsoleLocale:   Locale used in vCenter VM web console. By default, script uses en_US locale for the web console locale. 
#                                      VcenterWebConsoleLocale parameter is optional.
# List of input parameters
param(
    [String] $VcenterHost,
    [String] $VcenterPort,
    [String] $VcenterUser,
    [String] $VcenterPwd,
    [String] $VcenterWebConsolePort,    
    [String] $VmId,
    [String] $VcenterWebConsoleLocale
)

# Force script to stop on non-terminating errors.
$ErrorActionPreference = "Stop"

# Validate required parameters
if ($VcenterHost) {
	$VcenterHost = $VcenterHost.trim()
}
if ([string]::IsNullOrEmpty($VcenterHost)) {
	$errorMessage = "VcenterHost is a required parameter. Cannot be null or empty or have only spaces"
    $results = "scriptReturnCode`:-1,vmWebConsoleURL`:,scriptErrorMessage`:$errorMessage"
    return $results 
}

if ($VcenterUser) {
	$VcenterUser = $VcenterUser.trim()
}
if ([string]::IsNullOrEmpty($VcenterUser)) {
	$errorMessage = "VcenterUser is a required parameter. Cannot be null or empty or have only spaces"
    $results = "scriptReturnCode`:-1,vmWebConsoleURL`:,scriptErrorMessage`:$errorMessage"
    return $results 
}

if ($VcenterPwd) {
	$VcenterPwd = $VcenterPwd.trim()
}
if ([string]::IsNullOrEmpty($VcenterPwd)) {
	$errorMessage = "VcenterPwd is a required parameter. Cannot be null or empty or have only spaces"
    $results = "scriptReturnCode`:-1,vmWebConsoleURL`:,scriptErrorMessage`:$errorMessage"
    return $results 
}

if ($VmId) {
	$VmId = $VmId.trim()
}
if ([string]::IsNullOrEmpty($VmId)) {
	$errorMessage = "VmId is a required parameter. Cannot be null or empty or have only spaces"
    $results = "scriptReturnCode`:-1,vmWebConsoleURL`:,scriptErrorMessage`:$errorMessage"
    return $results 
}

# Constants
Set-Variable VCENTER_PORT -option Constant -value "443"
Set-Variable VCENTER_WEB_CONSOLE_PORT -option Constant -value "9443"
Set-Variable VCENTER_WEB_CONSOLE_LOCALE -option Constant -value "en_US"
Set-Variable VCENTER_PROTOCOL -option Constant -value "https"

# Get SSL Thumbprint from vCenter Server URL
Function Get-SSLThumbprint {
    param(
    [Parameter(
        Position=0,
        Mandatory=$true,
        ValueFromPipeline=$true,
        ValueFromPipelineByPropertyName=$true)
    ]
    [Alias('FullName')]
    [String]$URL
    )
	
	if ("IDontCarePolicy" -as [type]) {
	} else {
    add-type @"
        using System.Net;
        using System.Security.Cryptography.X509Certificates;

            public class IDontCarePolicy : ICertificatePolicy {
            public IDontCarePolicy() {}
            public bool CheckValidationResult(
                ServicePoint sPoint, X509Certificate cert,
                WebRequest wRequest, int certProb) {
                return true;
            }
            }
"@
    }
    [System.Net.ServicePointManager]::CertificatePolicy = new-object IDontCarePolicy
    Invoke-RestMethod -Uri $URL -Method Get | Out-Null

    $webRequest = [System.Net.Webrequest]::Create("$URL")
    $sslThumbprint = $webRequest.ServicePoint.Certificate.GetCertHashString()

    return $sslThumbprint -replace '(..(?!$))','$1:'
}

# Validate VCenterPort
if ($PSBoundParameters.ContainsKey('VcenterPort')) {
    $portPattern = "^[0-9]+$"
    if (-Not ($VcenterPort -match  $portPattern)){
        $errorMessage = "Supplied VcenterPort value of $VcenterPort must contain numbers only."
        # Return error message    
        $results = "scriptReturnCode`:-1,vmWebConsoleURL`:,scriptErrorMessage`:$errorMessage"
        return $results
    }     
}
else {
	$VcenterPort = $VCENTER_PORT
}


# Validate VcenterWebConsolePort
if ($PSBoundParameters.ContainsKey('VcenterWebConsolePort')) {
    $portPattern = "^[0-9]+$"
    if (-Not ($VcenterWebConsolePort -match  $portPattern)){
        $errorMessage = "Supplied VcenterWebConsolePort value of $VcenterWebConsolePort must contain numbers only."
        # Return error message    
        $results = "scriptReturnCode`:-1,vmWebConsoleURL`:,scriptErrorMessage`:$errorMessage"
        return $results
    }     
}
else {
	$VcenterWebConsolePort = $VCENTER_WEB_CONSOLE_PORT
}


# Validate VcenterWebConsoleLocale
if ($PSBoundParameters.ContainsKey('VcenterWebConsoleLocale')) {
    $localePattern = "^[A-Za-z_]+$"
    if (-Not ($VcenterWebConsoleLocale -match  $localePattern)){
        $errorMessage = "Supplied VcenterWebConsoleLocale value of $VcenterWebConsoleLocale must contain alphabets and underscore only."
        # Return error message    
        $results = "scriptReturnCode`:-1,vmWebConsoleURL`:,scriptErrorMessage`:$errorMessage"
        return $results
    }     
}
else {
	$VcenterWebConsoleLocale = $VCENTER_WEB_CONSOLE_LOCALE
}


try{
    
    # Set powershell CLI configuration
    $null = Set-PowerCLIConfiguration -Scope User -DefaultVIServerMode Single -Confirm:$false
    $null = Set-PowerCLIConfiguration -Scope User -ParticipateInCEIP $false -Confirm:$false
	$null = Set-PowerCLIConfiguration -InvalidCertificateAction Ignore -Confirm:$false
    
    # Connect to vCenter
    $vCenterConnection = Connect-VIServer -Server $VcenterHost -Port $VcenterPort -Protocol $VCENTER_PROTOCOL -User $VcenterUser -Password $VcenterPwd

    # Get VM name
    $vmIdPS = "VirtualMachine-" + $VmId 
    $vmObject = Get-VM -Id $vmIdPS
    $vmName = $vmObject.Name

    # Get vCenter Instance UUID
    $vCenterInstanceUUID = $global:DefaultVIServer.InstanceUuid

    # Get vCenter name
    $vCenterName = $global:DefaultVIServer.Name

    # Get vCenter session ticket
    $sessionMgr = Get-View $DefaultViserver.ExtensionData.Content.SessionManager
    $sessionTicket = $sessionMgr.AcquireCloneTicket()

    # Get vCenter SSL Thumbprint
    $vCenterSSLThumbprint = Get-SSLThumbprint "https://$vCenterName"

    # Build vCenter VM Web Console URL
    $vmWebConsoleURL = "$VCENTER_PROTOCOL`://$vCenterName`:$vCenterWebConsolePort/vsphere-client/webconsole.html?vmId=$VmId&amp;vmName=$vmName&amp;serverGuid=$vCenterInstanceUUID&amp;locale=$VcenterWebConsoleLocale&amp;host=$vCenterName`:$VcenterPort&amp;sessionTicket=$sessionTicket&amp;thumbprint=$vCenterSSLThumbprint"

    # Return VM Web Console URL
    $results = "scriptReturnCode`:0,vmWebConsoleURL`:$vmWebConsoleURL,scriptErrorMessage`:"
    return $results
}
catch
{
    $errorMessage = $_.Exception.Message
    if ($errorMessage)
    {
        $errorMessage = $errorMessage.trim()
    }
    
    # Return error message    
    $results = "scriptReturnCode`:-1,vmWebConsoleURL`:,scriptErrorMessage`:$errorMessage"
    return $results 
}</code></pre>
