{
	-trees
		"/Home"
	-exclude
		"/Home/A*"
		"/Home/B*"
		"/Home/C*"
		"/Home/D*"
		"/Home/E*"
		"/Home/F*"
		"/Home/G*"
		"/Home/H*"
		"/Home/I*"
		"/Home/J*"
		"/Home/K*"
		"/Home/L*"
		"/Home/M*"
		"/Home/N*"
		"/Home/O*"
		"/Home/P*"
		"/Home/Q*"
		"/Home/R*"
		"/Home/S*"
		"/Home/T*"
		"/Home/U*"
		"/Home/V*"
		"/Home/W*"
		"/Home/X*"
		"/Home/Y*"
		"/Home/Z*"
                "/$Recycle.Bin"
} 

