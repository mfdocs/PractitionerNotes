<p>Configure database to startup on machine startup</p>

<p>/etc/oratab</p>

<pre>truth:/u01/app/o<strong>rac</strong>le/product/12.1.0/dbhome_1:Y</pre>

<p>/etc/init.d/opsware-oracle</p>

<pre>#
# chkconfig: 235 60 20
# description: O<strong>rac</strong>le12c 12.1.0.2
#
### BEGIN INIT INFO
# Provides: opsware-o<strong>rac</strong>le
# Required-Start: $network
# Required-Stop: $network
# Default-Start: 2 3 5
# Default-Stop: 0 1 4 6
# Description: O<strong>rac</strong>le Database startup script
### END INIT INFO
LOCKFILEPATH=/var/lock/subsys
LOCKFILE=o<strong>rac</strong>le
O<strong>RAC</strong>LE_HOME=/u01/app/o<strong>rac</strong>le/product/12.1.0/dbhome_1
LOG=/var/log/ora_initd_startup.log

# For SELinux support we need to use runuser
# This is not possible on SLES 11 or lower, so we'll have to default to su
SWITCH_USER_CMD=su
if [ "`rpm -qa | grep sles-release | wc -l`" -eq 0 -a -x /sbin/runuser ]; then
        SWITCH_USER_CMD=/sbin/runuser
fi
                                                                                              
start(){
        echo "$0: starting up..." &gt;&gt; $LOG
        date &gt;&gt; $LOG
        echo -n $"Starting O<strong>rac</strong>le Database: "
                                                                                              
        # The listener starts as part of dbstart
        $SWITCH_USER_CMD - o<strong>rac</strong>le -c "env O<strong>RAC</strong>LE_HOME=$O<strong>RAC</strong>LE_HOME $O<strong>RAC</strong>LE_HOME/bin/dbstart $O<strong>RAC</strong>LE_HOME"

        # This is specific to Linux OS
        if [[ ! -d "${LOCKFILEPATH}" ]]; then
                mkdir -p $LOCKFILEPATH
        fi
        touch $LOCKFILEPATH/$LOCKFILE

echo "$0: starting up complete..." &gt;&gt; $LOG
date &gt;&gt; $LOG
echo "Starting O<strong>rac</strong>le Database complete "
}
stop(){
        echo "$0: shutting down" &gt;&gt; $LOG
        date &gt;&gt; $LOG
        echo -n $"Stopping O<strong>rac</strong>le Database: "
                                                                                              
        $SWITCH_USER_CMD - o<strong>rac</strong>le -c "env O<strong>RAC</strong>LE_HOME=$O<strong>RAC</strong>LE_HOME $O<strong>RAC</strong>LE_HOME/bin/dbshut $O<strong>RAC</strong>LE_HOME"

        # This is specific to Linux
        if [ -f $LOCKFILEPATH/$LOCKFILE ]; then
                rm -f $LOCKFILEPATH/$LOCKFILE
        fi

       echo "$0: shutdown complete..." &gt;&gt; $LOG
       date &gt;&gt; $LOG
       echo "Shutting O<strong>rac</strong>le Database complete "
}
restart(){
   stop
   start
}
case "$1" in
'start')
        start
        ;;
'stop')
        stop
        ;;
'restart')
        restart
   ;;
'status')
        if [ -f $LOCKFILEPATH/$LOCKFILE ]; then
                echo "O<strong>rac</strong>le Database instance running"
        else
                echo "O<strong>rac</strong>le Database instance not running"
        fi
   ;;
                                                                                              
*)
        echo $"Usage: $0 {start|stop|restart|status}"
        exit 1
        ;;
esac</pre>

