bootloader --location=mbr
firewall --disabled
install
keyboard us
lang en_US.UTF-8
timezone --utc UTC
cdrom
text

# Partition information
clearpart --all --initlabel
#autopart
part /boot --fstype ext3 --size=200
part swap --size=1024
part pv.01 --size=1 --grow --ondisk=sda
volgroup vg_root pv.01
logvol  /  --vgname=vg_root  --size=1 --grow --name=lv_root

reboot
rootpw opsware
skipx
text
zerombr
selinux --disabled

%packages
@Base
# O<strong>rac</strong>le pre-requisite packages
# https://docs.o<strong>rac</strong>le.com/database/121/LTDQI/toc.htm#BABGGEBA
binutils 
compat-libcap1
# Not on the media
#compat-libstdc++-33
gcc
gcc-c++
glibc
glibc-devel
ksh
libaio
libaio-devel
libgcc
libstdc++
libstdc++-devel
libXi
libXtst
make
sysstat
# extra
nfs-utils
autofs
screen
%end

%post --log=/root/ks-post.log

cat &gt;&gt; /etc/sysctl.conf &lt;&lt;EOF
#O<strong>rac</strong>le parameters begin
kernel.shmmax=4101414912
kernel.shmall=2097152
kernel.shmmni=4096
kernel.sem=256 32000 256 256
net.core.rmem_default=262144
