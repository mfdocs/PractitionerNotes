systemctl enable autofs
systemctl start autofs

/usr/sbin/groupadd oinstall
/usr/sbin/groupadd dba
/usr/sbin/useradd -g oinstall -G dba o<strong>rac</strong>le

mkdir -p /u01/app/o<strong>rac</strong>le/admin/truth/scripts
mkdir -p /u02/oradata/truth /u03/oradata/truth /u04/oradata/truth
chown -R o<strong>rac</strong>le:oinstall /u01/app /u02/oradata /u03/oradata /u04/oradata

cat &gt;&gt; /home/o<strong>rac</strong>le/.bashrc &lt;&lt;EOF
# Uncomment to setup O<strong>RAC</strong>LE_HOME on login
#export O<strong>RAC</strong>LE_SID=truth
#ORAENV_ASK=NO . /usr/local/bin/oraenv
#export O<strong>RAC</strong>LE_HOME_LISTNER=$O<strong>RAC</strong>LE_HOME
EOF

cat &gt;&gt; /root/.screenrc &lt;&lt;EOF
startup_message off
vbell on
hardstatus on
hardstatus alwayslastline
hardstatus string '%{= Kd} %{= Kd}%-w%{= Kr}[%{= KW}%n %t%{= Kr}]%{= Kd}%+w %-= %{KG} %H%{KW}|%{KY}%l%{KW}|%D %M %d %Y%{= Kc} %C%A%{-}'
nonblock on
escape ^Oo
bind ^x
bind x
attrcolor b ".I"
attrcolor i "+b"
defhstatus "^EH"
altscreen on
EOF

yum -y install compat-libstdc++-33
%end</pre>

